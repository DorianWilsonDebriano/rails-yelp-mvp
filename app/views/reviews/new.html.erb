<div class="container">
  <h3>Add a review</h3>
  <%= simple_form_for([@restaurant, @review]) do |f| %>
    <%= f.input :rating, input_html: { min:0, max: 5 } %>
    <%= f.input :content %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
  <br>
  <%= link_to 'Back', restaurants_path %>
</div>
